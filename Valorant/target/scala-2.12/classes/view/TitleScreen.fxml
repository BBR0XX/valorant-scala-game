<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="valorant.view.TitleController">
   <children>
      <Pane prefHeight="500.0" prefWidth="900.0">
         <children>
            <ImageView fx:id="titleBackground" fitHeight="500.0" fitWidth="900.0" pickOnBounds="true" />
            <ButtonBar layoutX="556.0" layoutY="283.0" prefHeight="54.0" prefWidth="242.0">
               <buttons>
                  <Button minWidth="0.0" mnemonicParsing="false" onAction="#handlePlay" prefHeight="40.0" prefWidth="135.0" text="Play">
                     <font>
                        <Font size="14.0" />
                     </font></Button>
                  <Button mnemonicParsing="false" onAction="#handleExit" prefHeight="40.0" text="Exit">
                     <font>
                        <Font size="14.0" />
                     </font></Button>
               </buttons>
            </ButtonBar>
         </children>
      </Pane>
   </children>
</AnchorPane>
